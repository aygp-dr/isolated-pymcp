{
    "jsonrpc": "2.0",
    "method": "textDocument/didChange",
    "params": {
        "textDocument": {
            "uri": "file:///path/to/file.py",
            "version": 2
        },
        "contentChanges": [
            {
                "range": {
                    "start": {"line": 1, "character": 4},
                    "end": {"line": 1, "character": 10}
                },
                "text": "transform"
            }
        ]
    }
}
